<h3>{{username}}'s Profile</h3>
<p><strong>Favorite Bars</strong></p>
<ul class="list-unstyled">
  {{#each bars}}
  <li><a href="/users/{{../username}}/bars/{{this._id}}">{{this.name}}</a></li>
  {{/each}}
</ul>
{{#if isOwner}}
<hr>
<a class="btn btn-default" href="/users/{{username}}/bars/new" type="button" name="button">Add Bar</a>
{{/if}}
